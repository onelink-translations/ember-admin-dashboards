{{! This file is a modification of html from https://github.com/almasaeed2010/AdminLTE}}

        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            {{t 'trans.timeline.usertitle' user='Jack'}}
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> {{t 'trans.navigation.breadcrumbs.home'}}</a></li>
            <li><a href="#">{{t 'trans.navigation.breadcrumbs.ui'}}</a></li>
            <li class="active">{{t 'trans.navigation.breadcrumbs.timeline'}}</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">

          <!-- row -->
          <div class="row">
            <div class="col-md-12">
              <!-- The time line -->
              <ul class="timeline">
                <!-- timeline time label -->
                <li class="time-label">
                  <span class="bg-red">
                    4 {{t 'trans.time.date.apr'}} 2017
                  </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-envelope bg-blue"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>
                    <h3 class="timeline-header"><a href="#">Support Team</a> {{t 'trans.timeline.user.email'}}</h3>
                    <div class="timeline-body">
                      Thank you for your inquiry.  We're looking into the matter now. Do you have any addition information you can provide? If so, it would help us solve the issue quicker.  Please attach a bug report and email...
                    </div>
                    <div class='timeline-footer'>
                      <a class="btn btn-primary btn-xs">{{t 'trans.timeline.user.readmore'}}</a>
                      <a class="btn btn-danger btn-xs">{{t 'trans.timeline.user.dele'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-user bg-aqua"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.second' count=35}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header no-border"><a href="#">Sarah Young</a> {{t 'trans.timeline.user.friend'}}</h3>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-comments bg-yellow"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.minute' count=25}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Jay White</a> {{t 'trans.timeline.user.comment'}}</h3>
                    <div class="timeline-body">
                      Sounds good to me, can't wait to see it. Make sure you're ready for the game on Friday!
                    </div>
                    <div class='timeline-footer'>
                      <a class="btn btn-warning btn-flat btn-xs">{{t 'trans.timeline.user.view'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline time label -->
                <li class="time-label">
                  <span class="bg-green">
                    3 {{t 'trans.time.date.jan'}} 2014
                  </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-camera bg-purple"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.day.day' count=2}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Mina Lee</a> {{t 'trans.timeline.user.photo' count=4}}</h3>
                    <div class="timeline-body">
                      <img src="dist/img/phone-fun.jpg" alt="..." class='margin' style="width:auto;height:auto;max-width:225px;max-height:150px;display:inline-block" />
                      <img src="dist/img/menu-fun.jpg" alt="..." class='margin' style="width:auto;height:auto;max-width:225px;max-height:150px;display:inline-block"/>
                      <img src="dist/img/buildings-fun.jpg" alt="..." class='margin' style="width:auto;height:auto;max-width:225px;max-height:150px;display:inline-block"/>
                      <img src="dist/img/cats-fun.jpg" alt="..." class='margin' style="width:auto;height:auto;max-width:225px;max-height:150px;display:inline-block"/>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-link bg-green"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.day.day' count=2}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Elliot Anderson</a> {{t 'trans.timeline.user.link'}}</h3>
                    <div class="timeline-body">
                      <h3><a href="https://emberjs.com/">{{t 'trans.timeline.embertitle'}}</a></h3>
                      <p>{{t 'trans.timeline.emberblurb'}}</p>
                      <a href="https://emberjs.com/"><img src="dist/img/ember-fun.jpg" alt="..." class='margin' style="width:auto;height:auto;max-width:300px;max-height:200px;display:block"/></a>
                      <h6 font-size=".5em">{{t 'trans.timeline.ember'}}</h6>
                    </div>
                    <div class='timeline-footer'>
                      <a class="fa fa-thumbs-o-up">&nbsp;{{t 'trans.timeline.like'}}  &nbsp;&nbsp;&nbsp;</a>
                      <a class="fa fa-thumbs-o-down">&nbsp;{{t 'trans.timeline.dislike'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <li>
                  <i class="fa fa-clock-o bg-gray"></i>
                </li>
              </ul>
            </div><!-- /.col -->
          </div><!-- /.row -->



        </section><!-- /.content -->
